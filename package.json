{
  "name": "@datawheel/olap-client",
  "version": "2.0.0-beta.2",
  "description": "A multiclient library to handle requests with mondrian-rest and tesseract servers",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./types/index.d.ts",
  "readme": "README.md",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build:library": "rollup --config rollup.config.js --environment NODE_ENV:production,BABEL_ENV:production",
    "build:typings": "tsc --esModuleInterop --emitDeclarationOnly --declaration --project .",
    "build": "npm run build:library && npm run build:typings",
    "dev": "rollup --config rollup.config.js --watch --environment NODE_ENV:development,BABEL_ENV:development",
    "format": "prettier --write src/*.ts",
    "prepublishOnly": "npm run build && mocha",
    "pretest": "rollup --config rollup.config.js --environment NODE_ENV:test,BABEL_ENV:test",
    "test": "mocha"
  },
  "author": "Francisco Abarzua <francisco@datawheel.us>",
  "license": "MIT",
  "homepage": "https://github.com/Datawheel/olap-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/Datawheel/olap-client.git"
  },
  "bugs": {
    "url": "https://github.com/Datawheel/olap-client/issues"
  },
  "keywords": [
    "dataset",
    "datawheel",
    "olap",
    "tesseract-olap",
    "tesseract-server",
    "mondrian-rest",
    "mondrian-rest-client"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "form-urldecoded": "^1.0.0",
    "form-urlencoded": "^6.0.4",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@types/url-join": "^4.0.0",
    "mocha": "^8.3.2",
    "rollup": "^2.40.0",
    "rollup-plugin-cleanup": "^3.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-license": "^2.3.0",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "prettier": {
    "arrowParens": "avoid"
  }
}
